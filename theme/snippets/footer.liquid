<footer id="footer-content" class="footer">
  <div class="container">
    {% if has_footer %}
    <div class="row">
      {% assign footer_columns = settings.footer-columns | plus: 0 %}
      {% for i in (1..footer_columns) %}
      {% include '_footer-columns' with footer_columns %}
      {% endfor %}
    </div>
    {% endif %}
    
  </div>

{% if has_footer_navbar %}{% include 'navbar' with 'footer' %}{% endif %}
</footer>